# top-most EditorConfig file
root = true

# Defaults for all files
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

# Things with two spaces instead of 4
[*.{css,less,json,yaml,yml}]
indent_size = 2

[.eslintrc]
indent_size = 2